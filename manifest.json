{
	"content_scripts": [{
		"js": ["jquery.js","init.js"],
		"matches":["*://twitter.com/*"]
	}],
	"manifest_version":2,
	"name":"Descarga fotos Twitter",
	"version":"1.0",
	"description":"Permite descargar la foto de Twitter que est√°s viendo",
	"author":"zetogk",
	"permissions":["http://www.twitter.com", "http://*.twitter.com", "https://www.twitter.com", "https://*.twitter.com"],
	"web_accessible_resources":["init.js","jquery.js","script.js","styles.css"],
	"browser_action": {
      "default_icon": "icon.png"
  	}
}